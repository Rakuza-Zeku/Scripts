Play.Name = "Play"
Play.Parent = MusicPlayer_2
Play.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Play.Position = UDim2.new(-0.317073166, 0, 2.00952387, 0)
Play.Size = UDim2.new(0, 82, 0, 50)
Play.Style = Enum.ButtonStyle.RobloxButton
