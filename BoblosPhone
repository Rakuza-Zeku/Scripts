Play.TextWrapped = true

Stop.Name = "Stop"
Stop.Parent = MusicPlayer_2
Stop.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Stop.Position = UDim2.new(0.642276406, 0, 2.00952387, 0)
Stop.Size = UDim2.new(0, 82, 0, 50)
Stop.Style = Enum.ButtonStyle.RobloxButton
