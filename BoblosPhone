Stop.TextWrapped = true

Exit.Name = "Exit"
Exit.Parent = MusicPlayer_2
Exit.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Exit.Position = UDim2.new(-0.471544743, 0, -1.20952368, 0)
Exit.Size = UDim2.new(0, 58, 0, 34)
Exit.Style = Enum.ButtonStyle.RobloxButton
Exit.Font = Enum.Font.Fondamento
Exit.Text = "Exit"
Exit.TextColor3 = Color3.fromRGB(104, 83, 40)
Exit.TextScaled = true
Exit.TextSize = 14.000
Exit.TextWrapped = true

Sad.SoundId = "rbxassetid://1838260553"
Pirate.SoundId = "rbxassetid://1845164089"
Sad.Name = "Sad"
Pirate.Name = "Pirate"

Up.MouseButton1Click:Connect(function()
	Sad.Volume = Sad.Volume + 1
	Pirate.Volume = Pirate.Volume + 1
end)

Down.MouseButton1Click:Connect(function()
	Sad.Volume = Sad.Volume - 1
	Pirate.Volume = Pirate.Volume - 1
end)

Off.MouseButton1Click:Connect(function()
BoblosPhone:Destroy()
end)

Exit.MouseButton1Click:Connect(function()
	MusicApp.Visible = false
	Screen.Visible = true
end)

Play.MouseButton1Up:Connect(function()
	if Player.Text == "Sad" then
		Sad:Play()
	elseif Player.Text == "Pirate" then
		Pirate:Play()
		else return
	end
end)

Stop.MouseButton1Up:Connect(function()
	if Player.Text == "Sad" then
		Sad:Stop()
	elseif Player.Text == "Pirate" then
		Pirate:Stop()
	else return
	end
end)

MusicPlayer.MouseButton1Up:Connect(function()
	MusicApp.Visible = true
	Screen.Visible = false
end)
