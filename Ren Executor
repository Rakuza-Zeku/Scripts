-- Scripts:

local function KPWDG_fake_script() -- Sit.LocalScript 
	local script = Instance.new('LocalScript', Sit)

	script.Parent.MouseButton1Up:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.Sit = true
	end)
	
end
coroutine.wrap(KPWDG_fake_script)()
local function RXXB_fake_script() -- CustomStand.LocalScript 
	local script = Instance.new('LocalScript', CustomStand)

	script.Parent.MouseButton1Up:Connect(function()
		game.Players.LocalPlayer.Character.StandMorph.AnimationController:Destroy()
		game.Players.LocalPlayer.Character.StandMorph.LowerTorso:Destroy()
	end)
end
coroutine.wrap(RXXB_fake_script)()
local function IEBGSE_fake_script() -- InfYield.LocalScript 
	local script = Instance.new('LocalScript', InfYield)

	script.Parent.MouseButton1Up:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
	end)
	
end
coroutine.wrap(IEBGSE_fake_script)()
local function HCMZV_fake_script() -- Remington870ModGunPrisonLife.LocalScript 
	local script = Instance.new('LocalScript', Remington870ModGunPrisonLife)

	local player = game:GetService"Players".LocalPlayer
	local char = player.Character
	local gun = char["Remington 870"]
	local module = require(gun.GunStates)
	
	module.FireRate = 0.000000000000000001
	module.Ammo = 15
	module.ReloadTime = 0.8
	module.Bullet = 100
end
coroutine.wrap(HCMZV_fake_script)()
local function AEECWWT_fake_script() -- Dex.LocalScript 
	local script = Instance.new('LocalScript', Dex)

	script.Parent.MouseButton1Up:Connect(function()
		local rng = Random.new()
	
		local charset = {}
		for i = 48,  57 do table.insert(charset, string.char(i)) end
		for i = 65,  90 do table.insert(charset, string.char(i)) end
		for i = 97, 122 do table.insert(charset, string.char(i)) end
		local function RandomCharacters(length)
			if length > 0 then
				return RandomCharacters(length - 1) .. charset[rng:NextInteger(1, #charset)]
			else
				return ""
			end
		end
	
		local Dex = game:GetObjects("rbxassetid://9553291002")[1]
		Dex.Name = RandomCharacters(rng:NextInteger(5, 20))
		Dex.Parent = game:GetService("CoreGui")
	
		local function Load(Obj, Url)
			local function GiveOwnGlobals(Func, Script)
				local Fenv = {}
				local RealFenv = {script = Script}
				local FenvMt = {}
				FenvMt.__index = function(a,b)
					if RealFenv[b] == nil then
						return getfenv()[b]
					else
						return RealFenv[b]
					end
				end
				FenvMt.__newindex = function(a, b, c)
					if RealFenv[b] == nil then
						getfenv()[b] = c
					else
						RealFenv[b] = c
					end
				end
				setmetatable(Fenv, FenvMt)
				setfenv(Func, Fenv)
				return Func
			end
	
			local function LoadScripts(Script)
				if Script.ClassName == "Script" or Script.ClassName == "LocalScript" then
					spawn(function()
						GiveOwnGlobals(loadstring(Script.Source, "=" .. Script:GetFullName()), Script)()
					end)
				end
				for i,v in pairs(Script:GetChildren()) do
					LoadScripts(v)
				end
			end
	
			LoadScripts(Obj)
		end
	
		Load(Dex)
	end)
	
end
coroutine.wrap(AEECWWT_fake_script)()
local function XBOCLL_fake_script() -- M9ModGunPrisonLife.LocalScript 
	local script = Instance.new('LocalScript', M9ModGunPrisonLife)

	local player = game:GetService"Players".LocalPlayer
	local char = player.Character
	local gun = char["M9"]
	local module = require(gun.GunStates)
	
	module.FireRate = 0.000000000000000001
	module.Ammo = 15
	module.ReloadTime = 0.8
	module.Bullet = 100
end
coroutine.wrap(XBOCLL_fake_script)()
local function XHMZ_fake_script() -- Executor.Executor Operator 
	local script = Instance.new('LocalScript', Executor)

	local Main = script.Parent.Main
	Main.Draggable = true
	local Input = Main.InputFrame.Input
	local Execute = Main.Execute
	local Clear = Main.Clear
	local Exit = Main.Exit
	
	game.StarterGui:SetCore("SendNotification",{Title = "Update",Text = "-Added Scripts Side",Duration = 5})
	
	Input.MouseEnter:Connect(function()
		if Input.Text ~= "" then return end
	Input.Text = "Type Your Code Here!"	
	end)
	
	Input.MouseLeave:Connect(function()
		if Input.Text ~= "Type Your Here!" then return end
		Input.Text = ""
	end)
	
	Execute.MouseButton1Up:Connect(function()
		loadstring(Input.Text)()
	end)
	
	Clear.MouseButton1Up:Connect(function()
		Input.Text = ""
	end)
	
	Exit.MouseButton1Up:Connect(function()
		Main:TweenSize(UDim2.new(0, 0,0, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Quint,5,true)
		wait(5)
		Main.Visible = false
	end)
	
	local player = game:GetService"Players".LocalPlayer
	
	player.Chatted:Connect(function(msg)
		if msg == "/e Enable Executor" then
			Main.Visible = true
			Main:TweenSize(UDim2.new(0, 829,0, 366),Enum.EasingDirection.InOut,Enum.EasingStyle.Quint,5,true)
		elseif msg == "/e Disable Executor" then
			Main:TweenSize(UDim2.new(0, 0,0, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Quint,5,true)
			wait(5)
			Main.Visible = false
		end
	end)
end
coroutine.wrap(XHMZ_fake_script)()
